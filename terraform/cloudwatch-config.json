    "metrics": {
      "append_dimensions": {
        "InstanceId": "${InstanceId}"
      },
      "metrics_collected": {
        "disk": {
          "measurement": ["used_percent"],
          "metrics_collection_interval": 60,
          "resources": ["/"]
        },
        "mem": {
          "measurement": ["used_percent"],
          "metrics_collection_interval": 60
        }
      }
    }
  }
  